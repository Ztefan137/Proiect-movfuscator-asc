{
    "instructions":[
        {
            "name": "loadv",
            "param": ["variable","offset","depth"],
            "template": [
                "mov $<variable>_if<depth>,%edi",
                "mov <offset>(%edi),%ebp",
                "mov %ebp,<variabila>"
            ]
        },
        {
            "name": "storev",
            "param": ["variable","offset","depth"],
            "template": [
                "mov $<variable>_if<depth>,%edi",
                "mov <variable>, %ebp",
                "mov %ebp,<offset>(%edi)"
            ]
        },
        {
            "name": "loadr",
            "param": ["register","offset","depth"],
            "except":[["register%","l/register"]],
            "template": [
                "mov $<register%>_if<depth>,%edi",
                "mov <register>, <offset>(%edi)"
            ]
        },
        {
            "name": "storer",
            "param": ["register","offset","depth"],
            "except":[["register%","l/register"]],
            "template": [
                "mov $<register%>_if<depth>,%edi",
                "mov <register>, <offset>(%edi)"
            ]
        },
