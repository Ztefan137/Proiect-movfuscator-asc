                "movzbl current_carry, %ebx",
                "movb (%edx, %ebx, 1), %al",
                "movb %al, temp_res+2",
                "movl ADD_CARRY_PTR(,%ecx, 4), %edx",
                "movb (%edx, %ebx, 1), %cl",
                "movb %cl, temp_c2",
                "movzbl temp_c1, %ecx",
                "movl OR_PTR(,%ecx, 4), %edx",
                "movzbl temp_c2, %ebx",
