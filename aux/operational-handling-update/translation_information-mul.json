                "### Byte 0 ###",
                "movzbl temp_dest_buf, %ecx",
                "movl XOR_PTR(,%ecx, 4), %edx",
                "movzbl temp_src_buf, %ebx",
                "movb (%edx, %ebx, 1), %al",
                "movb %al, temp_res",

                "### Byte 1 ###",
                "movzbl temp_dest_buf+1, %ecx",
                "movl XOR_PTR(,%ecx, 4), %edx",
                "movzbl temp_src_buf+1, %ebx",
                "movb (%edx, %ebx, 1), %al",
                "movb %al, temp_res+1",

                "### Byte 2 ###",
