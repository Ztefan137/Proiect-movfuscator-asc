storeifr <registru>,<offset>,<depth>:
  mov $<registru fara %>_if<depth>,%edi
  mov <registru>, <offset>(%edi)

storeifv <variabila>,<offset>,<depth>:
  mov $<variabila>_if<depth>,%edi
  mov <variabila>, %ebp
  mov %ebp,<offset>(%edi)

loadifr <registru>,<offset>,<depth>:
  mov $<registru fara %>_if<depth>,%edi
  mov <offset>(%edi),<registru>

loadifv <variabila>,<offset>,<depth>:
  mov $<variabila>_if<depth>,%edi
  mov <offset>(%edi),%ebp
  mov %ebp,<variabila>

cmprr <registru1>,<registru2>,<depth>:
  mov <registru1>,if_operand_x
  mov <regsitru2>,if_operand_y
  //codul cu skip il scriu mai tarziu

